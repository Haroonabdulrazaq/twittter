<div class="column is-one-quarter">
	<nav class="panel">
		<p class="panel-heading">Profile</p>
		<div class="panel-block">
			<article class="media">
				<div class="media-left">
                    <figure class="image is-64x64">
						<%= gravatar_image_tag(current_user.email, size: 64, alt: current_user.name) %>
					</figure>
				</div>
				<div class="media-content">
					<div class="content">
						<p>
							<strong><%= current_user.name %></strong><br />
							<small class="has-text-info"><%= current_user.username %></small>
						</p>
					</div>
				</div>
			</article>
		</div>
		<div class="panel-block">
			<div class="level is-mobile">
				<div class="level-item has-centered-text">
					<div>
						<p class="heading">Tweeets</p>
						<p class="title is-6 has-text-info"><%= current_user.tweeets.count %></p>
					</div>
				</div>
				<div class="level-item has-centered-text">
					<div>
						<p class="heading">Following</p>
						<p class="title is-6 has-text-info"> <%= current_user.tweeets.count  * 123%> </p>
					</div>
				</div>
				<div class="level-item has-centered-text">
					<div>
						<p class="heading">Followers</p>
						<p class="title is-6  has-text-info"> <%= current_user.tweeets.count  * 465%>K</p>
					</div>
				</div>
			</div>
		</div>
	</nav>

    <div class="box"> 
    <i class="has-text-info fa fa-twitter fa fa-3x "></i>
        <a class="box has-text-centered">
            <strong class="has-text-info">
             <i class="fa fa-home"></i>
             Home</strong>
        <a>
        <a class="box has-text-centered">
            <strong class="has-text-info">
             <i class="fa fa-hashtag"></i>
             Explore</strong>
        <a>
        <a class="box has-text-centered">
            <strong class="has-text-info has-text-centered">
             <i class="fa fa-bell"></i>
             Notification</strong>
        <a>
        <a class="box has-text-centered">
            <strong class="has-text-info has-text-centered">
             <i class="fa fa-envelope"></i>
             Messages</strong>
        <a
        <a class="box has-text-centered">
            <strong class="has-text-info has-text-centered">
             <i class="fa fa-bookmark"></i>
             Bookmark</strong>
        <a> 
        <a class="box has-text-centered">
            <strong class="has-text-info has-text-centered">
             <i class="fa fa-users"></i>
             Profile</strong>
        <a>
            <%= link_to "Tweeet" ,new_tweeet_path, class:"button is-right is-info has-text-white is-centered"%>
    </div>
</div>